{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Bricks - Chrome",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:4322/",
      "webRoot": "${workspaceFolder}/uu_jokesg01/"
    },
    {
      "name": "Debug Bricks - Edge",
      "type": "pwa-msedge",
      "request": "launch",
      "url": "http://localhost:4322/",
      "webRoot": "${workspaceFolder}/uu_jokesg01/"
    },
    {
      "name": "Debug Bricks - Firefox",
      "type": "firefox",
      "request": "launch",
      "url": "http://localhost:4322/",
      "webRoot": "${workspaceFolder}/uu_jokesg01/",
      "reAttach": true,
      "pathMappings": [
        {
          "url": "webpack:///",
          "path": "${workspaceFolder}/uu_jokesg01"
        }
      ]
    },
    {
      "name": "Debug Core - Chrome",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:4323/",
      "webRoot": "${workspaceFolder}/uu_jokesg01-core/"
    },
    {
      "name": "Debug Core - Edge",
      "type": "pwa-msedge",
      "request": "launch",
      "url": "http://localhost:4323/",
      "webRoot": "${workspaceFolder}/uu_jokesg01-core/"
    },
    {
      "name": "Debug Core - Firefox",
      "type": "firefox",
      "request": "launch",
      "url": "http://localhost:4323/",
      "webRoot": "${workspaceFolder}/uu_jokesg01-core/",
      "reAttach": true,
      "pathMappings": [
        {
          "url": "webpack:///",
          "path": "${workspaceFolder}/uu_jokesg01-core"
        }
      ]
    }
  ]
}
